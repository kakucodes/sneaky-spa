    "bech32": "^2.0.0",
    "bfj": "^7.0.2",
    "bootstrap": "^5.3.3",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "graphql.macro": "^1.4.2",
    "graz": "^0.1.25",
    "html-webpack-plugin": "^5.5.0",
